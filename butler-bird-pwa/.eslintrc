{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "react-app",
  "rules": {
    "react/react-in-jsx-scope": "off",
    "camelcase": "off",
    "react-hooks/exhaustive-deps": [1, { "enableDangerousAutofixThisMayCauseInfiniteLoops": true }],
    "jsx-a11y/anchor-is-valid": "off"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
